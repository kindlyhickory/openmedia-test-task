{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
      "no-param-reassign": ["error", { "props": false }],
      "import/prefer-default-export": "off",
      "react/jsx-props-no-spreading": "off"
    }
}
